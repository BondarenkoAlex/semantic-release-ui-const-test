{
  "name": "ui-const-test",
  "version": "5.0.0-feature-dev-8.10",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepack": "npm run build && npm run clean-publish",
    "prebuild": "node ./constants-builder.js",
